<div class="home" data-background="#C97164" data-color="#F9F1E7">
  <div class="home__wrapper">
    <div class="home__titles">
      <% collections.forEach(function(collection, index) { %>
        <div class="home__titles__label">
          <div class="home__titles__label__text"><%= `${home.data.collection} ${String(index + 1).padStart(2, '0')}` %></div>
        </div>
        <div class="home__titles__title">
          <div class="home__titles__title__text"><%= collection.data.title %></div>
        </div>
      <% }) %>
    </div>

    <div class="home__gallery">
      <% [0, 1].forEach(function(row) { %>
        <% home.data.gallery.forEach(function(media, index) { %>
          <figure class="home__gallery__media home__gallery__media--<%= (index % 5) + 1 %>">
            <img class="home__gallery__media__image" alt="<%= media.image.alt %>" data-src="<%= media.image.url %>">
          </figure>
        <% }) %>
      <% }) %>
    </div>

    <a class="home__link" data-animation="button" href="<%= home.data.collections %>">
      <span><%= home.data.button %></span>
      <svg class="home__link__icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 288 60">
        <path stroke="#fff" opacity="0.4" fill="none" d="M144,0.5c79.25,0,143.5,13.21,143.5,29.5S223.25,59.5,144,59.5S0.5,46.29,0.5,30S64.75,0.5,144,0.5z"/>
        <path stroke="#fff" fill="none" d="M144,0.5c79.25,0,143.5,13.21,143.5,29.5S223.25,59.5,144,59.5S0.5,46.29,0.5,30S64.75,0.5,144,0.5z"/>
      </svg>
    </a>
  </div>
</div>